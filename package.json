{
  "name": "@sushichan044/eslint-todo",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushichan044/eslint-todo.git"
  },
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "eslint-todo": "bin/eslint-todo.mjs"
  },
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "types": "./dist/cli.d.ts"
    },
    "./eslint": {
      "import": "./dist/eslint.mjs",
      "types": "./dist/eslint.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "test": "vitest",
    "eslint:inspect": "eslint --inspect-config",
    "publish": "pnpm publish --no-git-checks"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "dependencies": {
    "citty": "0.1.6",
    "defu": "6.1.4",
    "jiti": "2.4.2",
    "magicast": "0.3.5",
    "pathe": "2.0.2"
  },
  "devDependencies": {
    "@types/node": "22.13.0",
    "@virtual-live-lab/eslint-config": "2.2.16",
    "@virtual-live-lab/prettier-config": "2.0.15",
    "@virtual-live-lab/tsconfig": "2.1.16",
    "eslint": "9.19.0",
    "prettier": "3.4.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0",
    "unbuild": "3.3.1",
    "vitest": "3.0.4"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^23.0.0"
  }
}
