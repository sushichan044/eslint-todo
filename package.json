{
  "name": "@sushichan044/eslint-todo",
  "version": "0.3.0",
  "description": "A simple tool to gradually resolve a large number of ESLint violations.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushichan044/eslint-todo.git"
  },
  "bin": {
    "eslint-todo": "bin/eslint-todo.mjs"
  },
  "files": [
    "config-schema.json",
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config/index.d.mts",
      "import": "./dist/config/index.mjs"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.mts",
      "import": "./dist/eslint/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "build:json-schema": "vite-node scripts/json-schema.ts",
    "lint": "eslint --max-warnings 0 .",
    "format": "oxfmt",
    "format:ci": "oxfmt --check",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "eslint-config-inspector": "eslint --inspect-config",
    "pkg-pr-new": "pkg-pr-new publish --compact --comment=update --pnpm",
    "check": "pnpm run build && pnpm run lint && pnpm run format:ci && pnpm run typecheck && pnpm run test",
    "prepare": "ts-patch install --silent",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@sushichan044/eslint-config-array-resolver": "https://pkg.pr.new/@sushichan044/eslint-config-array-resolver@40",
    "comlink": "4.4.2",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "gunshi": "0.27.5",
    "hookable": "6.0.1",
    "jiti": "2.6.1",
    "klona": "2.0.6",
    "magicast": "0.5.1",
    "pathe": "2.0.3",
    "picomatch": "4.0.3",
    "tinyexec": "1.0.2",
    "typia": "11.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "@ryoppippi/unplugin-typia": "2.6.5",
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.10.9",
    "@types/picomatch": "4.0.2",
    "@virtual-live-lab/eslint-config": "2.3.1",
    "@virtual-live-lab/prettier-config": "2.0.22",
    "@virtual-live-lab/tsconfig": "2.1.21",
    "@vitest/eslint-plugin": "1.6.6",
    "eslint": "9.39.2",
    "eslint-plugin-import-access": "3.1.0",
    "eslint-plugin-unicorn": "62.0.0",
    "fs-extra": "11.3.3",
    "globals": "17.3.0",
    "oxfmt": "0.28.0",
    "pkg-pr-new": "0.0.63",
    "publint": "0.3.17",
    "release-it": "19.2.4",
    "release-it-pnpm": "4.6.6",
    "ts-patch": "3.3.0",
    "tsdown": "0.20.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "unplugin-unused": "0.5.7",
    "vite": "7.3.1",
    "vite-node": "5.3.0",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "engines": {
    "node": "^22.0.0 || ^24.0.0 || >=25.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "typia"
    ]
  }
}
