{
  "name": "@sushichan044/eslint-todo",
  "version": "0.2.6",
  "description": "A simple tool to gradually resolve a large number of ESLint violations.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushichan044/eslint-todo.git"
  },
  "bin": {
    "eslint-todo": "bin/eslint-todo.mjs"
  },
  "files": [
    "config-schema.json",
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config/index.d.mts",
      "import": "./dist/config/index.mjs"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.mts",
      "import": "./dist/eslint/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsdown",
    "build:json-schema": "vite-node scripts/json-schema.ts",
    "dev": "unbuild --stub",
    "lint": "eslint --max-warnings 0 .",
    "format": "oxfmt",
    "format:ci": "oxfmt --check",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "bench": "vitest bench",
    "bench:run": "vitest bench --run",
    "eslint:inspect": "eslint --inspect-config",
    "pkg-pr-new": "pkg-pr-new publish --compact --comment=update --pnpm",
    "check": "pnpm run build && pnpm run lint && pnpm run format:ci && pnpm run typecheck && pnpm run test:run",
    "prepare": "ts-patch install --silent",
    "prepublishOnly": "pnpm run build",
    "ai:prettier": "prettier --experimental-cli"
  },
  "dependencies": {
    "@sushichan044/eslint-config-array-resolver": "0.1.2",
    "comlink": "4.4.2",
    "consola": "3.4.2",
    "defu": "6.1.4",
    "gunshi": "0.27.0-alpha.10",
    "hookable": "5.5.3",
    "jiti": "2.5.1",
    "klona": "2.0.6",
    "magicast": "0.3.5",
    "pathe": "2.0.3",
    "picomatch": "4.0.3",
    "tinyexec": "1.0.1"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "@ryoppippi/unplugin-typia": "2.6.5",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.18.0",
    "@types/picomatch": "4.0.2",
    "@virtual-live-lab/eslint-config": "2.3.1",
    "@virtual-live-lab/prettier-config": "2.0.22",
    "@virtual-live-lab/tsconfig": "2.1.21",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/eslint-plugin": "1.6.4",
    "eslint": "9.39.2",
    "eslint-flat-config-utils": "2.1.4",
    "eslint-plugin-import-access": "3.1.0",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-typegen": "2.3.0",
    "fs-extra": "11.3.3",
    "fs-fixture": "2.11.0",
    "globals": "17.0.0",
    "oxfmt": "0.21.0",
    "pkg-pr-new": "0.0.62",
    "pnpm": "10.27.0",
    "publint": "0.3.16",
    "release-it": "19.2.2",
    "release-it-pnpm": "4.6.6",
    "rolldown": "1.0.0-beta.35",
    "ts-patch": "3.3.0",
    "tsdown": "0.15.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "typia": "9.7.2",
    "unplugin-unused": "0.5.6",
    "vite": "7.3.0",
    "vite-node": "3.2.4",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "engines": {
    "node": "^20.12.0 || ^22.0.0 || >=24.0.0"
  },
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "typia"
    ],
    "overrides": {
      "esbuild": "0.27.0"
    }
  }
}
