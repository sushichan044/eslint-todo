{
  "name": "@sushichan044/eslint-todo",
  "version": "0.2.1",
  "type": "module",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "config-schema.json"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.mjs",
      "types": "./dist/config/index.d.ts"
    },
    "./eslint": {
      "import": "./dist/eslint/index.mjs",
      "types": "./dist/eslint/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp/index.mjs",
      "types": "./dist/mcp/index.d.ts"
    }
  },
  "bin": {
    "eslint-todo": "bin/eslint-todo.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sushichan044/eslint-todo.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "unbuild",
    "build:json-schema": "vite-node scripts/json-schema.ts",
    "dev": "unbuild --stub",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "eslint:inspect": "eslint --inspect-config",
    "pkg-pr-new": "pkg-pr-new publish --compact --comment=update --pnpm",
    "check": "pnpm run build && pnpm run lint && pnpm run format:ci && pnpm run typecheck && pnpm run test:run",
    "prepare": "ts-patch install",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.12.1",
    "@typescript-eslint/utils": "8.33.0",
    "bundle-require": "5.1.0",
    "citty": "0.1.6",
    "comlink": "4.4.2",
    "consola": "3.4.2",
    "cross-spawn": "7.0.6",
    "defu": "6.1.4",
    "find-up": "7.0.0",
    "hookable": "5.5.3",
    "jiti": "2.4.2",
    "klona": "2.0.6",
    "magicast": "0.3.5",
    "minimatch": "10.0.1",
    "mlly": "1.7.4",
    "pathe": "2.0.3",
    "picomatch": "4.0.2",
    "tinyexec": "1.0.1",
    "zod": "3.25.56"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "2.6.4",
    "@samchon/openapi": "4.3.3",
    "@types/cross-spawn": "6.0.6",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.15.29",
    "@types/picomatch": "4.0.0",
    "@virtual-live-lab/eslint-config": "2.2.21",
    "@virtual-live-lab/prettier-config": "2.0.19",
    "@virtual-live-lab/tsconfig": "2.1.20",
    "@vitest/coverage-v8": "3.2.3",
    "@vitest/eslint-plugin": "1.2.1",
    "eslint": "9.28.0",
    "eslint-flat-config-utils": "2.1.0",
    "eslint-plugin-import-access": "3.0.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-typegen": "2.1.0",
    "fs-extra": "11.3.0",
    "globals": "16.2.0",
    "pkg-pr-new": "0.0.51",
    "pnpm": "10.12.1",
    "prettier": "3.5.3",
    "release-it": "19.0.3",
    "release-it-pnpm": "4.6.6",
    "ts-patch": "3.3.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0",
    "typia": "9.3.1",
    "unbuild": "3.5.0",
    "vite": "6.3.5",
    "vite-node": "3.2.3",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^23.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "typia"
    ],
    "overrides": {
      "esbuild": "0.25.5"
    },
    "patchedDependencies": {
      "find-up@7.0.0": "patches/find-up@7.0.0.patch"
    }
  }
}
